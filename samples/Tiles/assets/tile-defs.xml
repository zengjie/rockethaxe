<catalog title="Demo Tiles" pixelWidth="32" pixelHeight="32">

  <!--
      The TileCatalog XML format is described here:
      https://code.google.com/p/rockethaxe/wiki/TileCatalog
  -->

  <collisionTags>
    <land />
    <water />
  </collisionTags>

  <frames>
    <grid label="land" x="0" y="0" columns="16" rows="2"  />
  </frames>

  <tiles>
    <!-- Reverse the tilemap so 1 is land in the auto-tiler -->

    <!-- Sand/Water -->
    <tile label="sand" />
    <tile collision="water" />
    <tile collision="water" />
    <tile collision="water" />

    <tile collision="water" />
    <tile collision="water" />
    <tile collision="water" />
    <tile collision="water" />

    <tile collision="water" />
    <tile collision="water" />
    <tile collision="water" />
    <tile collision="water" />

    <tile collision="water" />
    <tile collision="water" />
    <tile collision="water" />
    <tile collision="water" label="water" />

    <!-- Grass/Sand -->
    <tile frame="31" label="unused" />
    <tile frame="30" />
    <tile frame="29" />
    <tile frame="28" />

    <tile frame="27" />
    <tile frame="26" />
    <tile frame="25" />
    <tile frame="24" />

    <tile frame="23" />
    <tile frame="22" />
    <tile frame="21" />
    <tile frame="20" />

    <tile frame="19" />
    <tile frame="18" />
    <tile frame="17" />
    <tile frame="16" label="grass" />
  </tiles>

</catalog>
