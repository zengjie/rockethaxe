<catalog title="Demo Tiles" width="32" height="32">

  <!--
      The TileCatalog XML format is described here:
      https://code.google.com/p/rockethaxe/wiki/TileCatalog
  -->

  <collisionTags>
    <land />
    <water />
    <deadly />
    <pit code="8" />
  </collisionTags>

  <frames>
    <grid label="land" x="0" y="0" columns="8" rows="8"  />
    <keyframe label="fire" frame="1" />
  </frames>

  <tiles>
    <tile label="land"      frame="land" collision="land" />
    <tile label="water"     frame="1"    collision="water" />
    <tile label="deepwater" frame="3"    collision="water,deadly" />
    <tile label="fire"      frame="fire" collision="4" />
    <tile label="pit"       frame="5"    collision="pit" />
  </tiles>

</catalog>
